		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.14",
		"@types/node": "^24",
		"@types/three": "^0.181.0",
		"drizzle-kit": "^0.31.5",
		"drizzle-orm": "^0.44.6",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
